{
  "name": "nodejs-service-template",
  "version": "1.0.0",
  "description": "A template for expressjs service using Typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/c9nguyen/nodejs-serice-typescript-template.git"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "npm": "^6.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.7",
    "@types/sinon": "^5.0.7",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "sinon": "^7.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --exec ts-node src/server.ts",
